{
  "name": "bike-shop",
  "version": "0.1.0",
  "description": "A simple bike shop made with vue deno and rollup bundle",
  "repository": "https://github.com/silverfang94/node-experiments.git",
  "author": "Ion Cretu <cretuion94@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev:client": "rollup -c -w",
    "dev:server": "denon run --allow-write --allow-read --allow-plugin --allow-net --allow-env server/app.ts",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
  "dependencies": {
    "concurrently": "^5.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.18.0",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "postcss-each": "^0.10.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "rollup": "^2.21.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-esbuild": "^2.1.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-vue": "^5.1.9",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.6",
    "typescript-deno-plugin": "^1.31.0",
    "vue-template-compiler": "^2.6.11"
  }
}
