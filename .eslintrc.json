{
  "env": {
    "browser": true,
    "es2020": false,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "no-console": "off",
    "import/prefer-default-export": "off",
    "import/extensions": [
      "always",
      {
        "ignorePackages": true
      }
    ]
  }
}
